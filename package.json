{
  "name": "majime",
  "version": "4.0.0",
  "description": "Collection of TS utility functions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/languagedrops/majime.git"
  },
  "keywords": [
    "typescript",
    "utility"
  ],
  "author": "Drops",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/languagedrops/majime/issues"
  },
  "homepage": "https://github.com/languagedrops/majime#readme",
  "devDependencies": {
    "@types/jest": "^23.0.0",
    "babel-jest": "^23.0.1",
    "jest": "^23.1.0",
    "ts-jest": "^22.4.6",
    "tslint": "5.9.1",
    "tslint-eslint-rules": "^4.1.1",
    "tslint-no-circular-imports": "^0.2.0",
    "tslint-react": "^3.2.0",
    "typescript": "2.8.1"
  },
  "dependencies": {},
  "jest": {
    "bail": true,
    "modulePaths": [
      "<rootDir>"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/(tests|e2e)/.*\\.(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "globals": {
      "ts-jest": {
        "useBabelrc": false
      }
    }
  }
}
